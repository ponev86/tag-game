{
  "exclude": ["node_modules"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "baseUrl": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "esnext"
  }
}
